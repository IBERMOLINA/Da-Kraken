{
  "name": "web-development-project",
  "version": "1.0.0",
  "description": "Modern web development project with HTML, CSS, and JavaScript",
  "main": "index.html",
  "scripts": {
    "start": "live-server --port=8080 --host=0.0.0.0",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "e2e": "cypress run",
    "e2e:open": "cypress open",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint src/**/*.{js,jsx,ts,tsx} --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,html}",
    "serve": "serve -s . -l 8080",
    "http-server": "http-server -p 8080 -c-1"
  },
  "keywords": [
    "web",
    "html",
    "css",
    "javascript",
    "frontend",
    "development"
  ],
  "author": "Web Developer",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cypress": "^13.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.0",
    "jest-environment-jsdom": "^29.6.0",
    "prettier": "^3.0.0",
    "typescript": "^5.1.0",
    "vite": "^4.4.0"
  },
  "dependencies": {},
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}