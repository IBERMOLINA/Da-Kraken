{
    "name": "javascript-dev-container",
    "version": "1.0.0",
    "description": "JavaScript development container with AI assistant and auto-dependency management",
    "main": "index.js",
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "lint": "eslint src/",
        "lint:fix": "eslint src/ --fix",
        "format": "prettier --write src/",
        "serve": "serve -s build",
        "dev": "vite",
        "preview": "vite preview",
        "ai-assistant": "node ai-assistant/chat-server.js"
    },
    "dependencies": {
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-scripts": "5.0.1",
        "web-vitals": "^4.2.4",
        "axios": "^1.7.9",
        "express": "^4.21.2",
        "socket.io": "^4.8.1",
        "cors": "^2.8.5"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.1.0",
        "@testing-library/user-event": "^14.5.2",
        "@types/react": "^19.1.3",
        "@types/react-dom": "^19.1.3",
        "@typescript-eslint/eslint-plugin": "^8.20.0",
        "@typescript-eslint/parser": "^8.20.0",
        "@vitejs/plugin-react": "^4.4.1",
        "eslint": "^9.17.0",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^5.1.0",
        "prettier": "^3.4.2",
        "typescript": "^5.7.3",
        "vite": "^7.1.7"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "engines": {
        "node": ">=20.0.0",
        "npm": ">=10.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/IBERMOLINA/Da-Kraken.git"
    },
    "keywords": [
        "react",
        "javascript",
        "development",
        "container",
        "ai-assistant",
        "auto-setup"
    ],
    "author": "Da-Kraken Team",
    "license": "MIT"
}